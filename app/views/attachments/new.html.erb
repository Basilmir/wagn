<% form_for current_object, :url=>objects_path, :html=>{ 
		:id=>'upload-form', :multipart=>true, :method=>:post, :name=>'fileupload' } do |f| %>    
  <%= f.hidden_field :type %> 
  <%= f.hidden_field :attachment_uuid %>    	
  <%= f.file_field :uploaded_data, :onchange =>"this.form.submit()" %>
