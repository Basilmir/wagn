<div class="setting-row not-set">
	<div class="value">      
		<% if card.ok?(:edit) %>		                       
			<span class="edit-link faint">
				<%#= slot.link_to_action('add','new') %>
				<%= link_to "add", "/card/new/#{card.name}" %>
			</span>
		<% end %>
	</div>
	<div class="set">
	  <%- set_name = card.name.trunk_name %>
	  <%= link_to_page Wagn::Pattern.label(set_name), set_name %>
	</div>
</div>